#include<iostream>
#include<vector>
using namespace std;

void Bubble_sort(vector<int>&v)  //We are passing the vector by reference so that any changes made can be reflected to the original vector
{
for(int i = 0;i<v.size() - 1;i++)
{
for(int j = 0;j<v.size() - 1 - j;j++)
{
if(v[j]>v[j + 1])
{
swap(v[j),v[j + 1])
}
}
}
}

void main()
{
int n;
cout<<"Enter the size of the vector: ";
cin>>n;

vector<int>v; //Initialising a vector
for(int i = 0;i<n;i++)
{
cin>>v[i];
}
Bubble_sort(v);

cout<<"The sorted vector is: "<<endl;
for(int i = 0;i<n;i++)
{
cout<<v[i];
}
}
